{
  "manifest_version": 3,
  "name": "UserAgent Extension",
  "version": "1.5.1",
  "description": "A simple Chrome UserAgent tool.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "icons": {
    "16": "images/icon16.png"
  }
}

